local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("PornHub | Player Misc", "GrapeTheme")

local Tab = Window:NewTab("Home")
local Section = Tab:NewSection("Credits")
Section:NewButton("Credits [Dev Console]", "Credits", function()
    print("No one </3")
end)
local Tab = Window:NewTab("Player")                         
local Section = Tab:NewSection("WalkSpeed")
Section:NewSlider("WalkSpeed", "WalkSpeed", 1000, 15, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
local Section = Tab:NewSection("JumpPower")
Section:NewSlider("JumpPower", "JumpPower", 1000, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
local Section = Tab:NewSection("Fly")
Section:NewButton("Fly", "Makes your character Fly...", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/7rXZ9VNc", true))()
end)
local Tab = Window:NewTab("Admin Scripts")
local Section = Tab:NewSection("Fate Admin")
Section:NewButton("Load", "Loads Fate Admin", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/fatesc/fates-admin/main/main.lua"))();
end)
local Section = Tab:NewSection("Infinite Yield")
Section:NewButton("Load", "Loads Infinite Yield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
local Section = Tab:NewSection("CMD-X")
Section:NewButton("Load", "Loads CMD-X", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end)
local Section = Tab:NewSection("Reviz Admin")
Section:NewButton("Load", "Loads Reviz Admin", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/pyzjWNhk'),true))()
end)
